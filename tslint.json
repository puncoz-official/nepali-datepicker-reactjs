{
  "defaultSeverity": "error",
  "extends":         [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules":         {
  },
  "rules":           {
    "member-access":   false,
    "ordered-imports": false,
    "quotemark":       false,
    "no-console":      false,
    "semicolon":       false,
    "jsx-no-lambda":   false,
    "prettier":        true,
    "interface-name":  [
      false,
      "never-prefix"
    ]
  },
  "rulesDirectory":  [
    "tslint-plugin-prettier"
  ],
  "linterOptions":   {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
